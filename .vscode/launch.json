{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter: Launch on Emulator (with Supabase keys)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "deviceId": "emulator-5554",
      "args": [
        "--dart-define=SUPABASE_URL=https://your-project.supabase.co",
        "--dart-define=SUPABASE_ANON_KEY=your_anon_key"
      ]
    },
    {
      "name": "Flutter: Run Chrome (web) (with Supabase keys)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "deviceId": "chrome",
      "args": [
        "--dart-define=SUPABASE_URL=https://your-project.supabase.co",
        "--dart-define=SUPABASE_ANON_KEY=your_anon_key"
      ]
    }
  ]
}
